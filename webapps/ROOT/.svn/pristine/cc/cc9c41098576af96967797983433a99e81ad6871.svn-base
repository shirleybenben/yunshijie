.order_list_w .order_top_l ul li{width:98px;}
.order_filter{width:100%;height:83px;background-color:#fff;}
.order_filter span.select{float:left;margin:48px 0 0 22px;font-size:12px;color:#999;}
ol.order_list{margin:0 22px;width:1156px;}
ul.filter_w{padding:46px 0 0 45px;-webkit-box-sizing: border-box;
-moz-box-sizing: border-box;
box-sizing: border-box;float:left;width:1134px;}
ul.filter_w li{float:left;margin-right:25px;}
ul.filter_w li span.pubdate{width:162px;height:28px;border:1px solid #dcdcdc;color:#505050;padding-left:14px;-webkit-box-sizing: border-box;
-moz-box-sizing: border-box;
box-sizing: border-box;display:block;line-height:28px;cursor:pointer;padding-right:12px;background:#eee url(../img/select_time.png) no-repeat 147px center;}
ul.filter_w li .class_2,ul.filter_w li .class_3,ul.filter_w li .needLength,ul.filter_w li .needType{width:162px;height:25px;border:1px solid #dcdcdc;outline:none;margin-top:2px;background-color:#eee;}
ul.filter_w li .needLength,ul.filter_w li .needType{width:83px;}
ul.filter_w li.back_source{padding:7px 0 0 15px;float:right;}
ul.filter_w li label input.returned{margin-right:10px;width:14px;height:14px;cursor:pointer;}
ol.order_list li{position:relative;}
ol.order_list li .quoted_price{width:65px;height:52px;position:absolute;top:80px;right:260px;outline:none;background:url(../img/quoted_price.png) no-repeat center;}
.order_list_w .order_top_l ul li{width:164px;padding:0 3px;}
.order_list_w .order_top_m{float:right;padding-right:24px;}
.order_list_w .order_top_m a{color:#fff;font-size:16px;text-decoration:none;padding-left:20px;background:url(../img/add_line.png) no-repeat left center;display:block;}