<div class="content_w wrap container_w">
	<div class="title clearfix">
		<div class="distance">
			<span>预计公里数</span>
			<em><em ng-bind="loadBillInfo.miles"></em> KM</em>
		</div>
		<!-- <div class="distance">
			<span>在途要求</span>
			<em>2H一次</em>
		</div>
		<a href="#">查看地图</a>-->
	</div>
	<div class="con">
		<div class="item clearfix">
			<p>
				<span>计划发车时间</span>
				<span class="font18" ng-bind="loadBillInfo.needStartTime"></span>
			</p>
			<p>
				<span>发货人</span>
				<span class="font18" ng-bind="loadBillInfo.contactName"></span>
			</p>
			<p>
				<span>发货人电话</span>
				<span class="font18">
					<em ng-bind="loadBillInfo.sendTelphone"></em>
					<em ng-bind="loadBillInfo.sendMobile"></em>
				</span>
			</p>
			<p>
				<span>发货地址</span>
				<span class="font18" ng-bind="loadBillInfo.senderAreaList | getCompArr"></span>
			</p>
		</div>
		<div class="item clearfix">
			<p>
				<span>到达时限</span>
				<span class="font18" ng-bind="loadBillInfo.needArriveTime"></span>
			</p>
			<p>
				<span>发货人</span>
				<span class="font18" ng-bind="loadBillInfo.receiveName"></span>
			</p>
			<p>
				<span>发货人电话</span>
				<span class="font18">
					<em ng-bind="loadBillInfo.receiverTelphone"></em>
					<em ng-bind="loadBillInfo.receiverMobile"></em>
				</span>
			</p>
			<p>
				<span>发货地址</span>
				<span class="font18" ng-bind="loadBillInfo.receiveAreaList | getCompArr"></span>
			</p>
		</div>
	</div>
</div>